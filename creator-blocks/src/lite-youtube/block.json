{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "creator-blocks/lite-youtube",
	"version": "1.0.0",
	"title": "Lite YouTube Embed",
	"category": "embed",
	"icon": "video-alt3",
	"description": "Fast loading YouTube embed with VideoObject Schema.",
	"attributes": {
		"videoUrl": {
			"type": "string",
			"default": ""
		},
		"videoId": {
			"type": "string",
			"default": ""
		},
		"title": {
			"type": "string",
			"default": ""
		},
		"description": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "creator-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}
